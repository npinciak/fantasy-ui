{
  "name": "sports-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4400",
    "build": "ng build",
    "generate:component": "cd schematics/component && tsc -p tsconfig.json && cd ../.. && npx schematics ./schematics/component/src/collection.json:component --debug=false",
    "generate:feature": "cd schematics/feature && tsc -p tsconfig.json && cd ../.. && npx schematics ./schematics/component/src/collection.json:feature --debug=false",
    "generate:state": "cd schematics/state && tsc -p tsconfig.json && cd ../.. && npx schematics ./schematics/state/src/collection.json:state --debug=false",
    "test": "ng test --configuration=develop --code-coverage --watch=true",
    "test:ci": "npm run lint && npm run test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "createEnv:Dev": "node ./scripts/configEnv.ts",
    "createEnv:Prod": "node ./scripts/configEnv.ts --environment=prod",
    "playground": "angular-playground"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.11",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/router": "~13.3.11",
    "@angular/service-worker": "~13.3.11",
    "@ngxs/router-plugin": "^3.7.4",
    "@ngxs/schematics": "0.0.1-alpha.5",
    "@ngxs/store": "^3.7.5",
    "angular-plotly.js": "^4.0.4",
    "lodash": "^4.17.21",
    "material-design-icons-iconfont": "^6.1.0",
    "ngx-skeleton-loader": "^6.0.0",
    "plotly.js-dist-min": "^2.11.1",
    "rxjs": "^6.6.6",
    "tslib": "^2.4.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.9",
    "@angular-devkit/schematics": "^13.3.9",
    "@angular-devkit/schematics-cli": "^0.1102.19",
    "@angular-eslint/builder": "1.2.0",
    "@angular-eslint/eslint-plugin": "1.2.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/schematics": "1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/cli": "~13.3.9",
    "@angular/compiler-cli": "~13.3.11",
    "@ngxs/devtools-plugin": "^3.7.1",
    "@types/jasmine": "~3.6.0",
    "@types/lodash": "^4.14.173",
    "@types/node": "^12.20.15",
    "@types/plotly.js-dist-min": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "codelyzer": "^6.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "2.3.2",
    "ts-node": "~8.3.0",
    "typescript": "^4.4.4"
  }
}
