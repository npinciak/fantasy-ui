{
  "name": "sports-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "nx",
    "start": "nx serve --port=4401",
    "build": "nx run build --configuration=production --skip-nx-cache ",
    "build:patch": "npm run bump:patch && npm run build",
    "build:minor": "npm run bump:minor && npm run build",
    "build:major": "npm run bump:major && npm run build",
    "bump:patch": "npm version patch",
    "bump:minor": "npm version minor",
    "bump:major": "npm version major",
    "test": "ng test --configuration=develop --code-coverage --watch=true",
    "test:ci": "npm run lint && npm run test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "install:sdk": "npm i ./libs/sports-ui-sdk",
    "circular-dependency": "node ./scripts/circular-dependency.ts",
    "fresh:install": "rm -rf .angular && rm -rf node_modules && npm i"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.0.0",
    "@angular/cdk": "15.0.0",
    "@angular/common": "15.0.0",
    "@angular/compiler": "15.0.0",
    "@angular/core": "15.0.0",
    "@angular/forms": "15.0.0",
    "@angular/material": "15.0.0",
    "@angular/platform-browser": "15.0.0",
    "@angular/platform-browser-dynamic": "15.0.0",
    "@angular/router": "15.0.0",
    "@angular/service-worker": "15.0.0",
    "@ngxs/router-plugin": "^3.8.0",
    "@ngxs/store": "^3.8.0",
    "@nrwl/angular": "16.2.2",
    "@supabase/supabase-js": "^2.2.0",
    "chart.js": "^3.8.0",
    "clone": "^2.1.2",
    "date-fns": "^2.29.3",
    "flowbite": "^1.6.3",
    "lodash": "^4.17.21",
    "material-design-icons-iconfont": "^6.1.0",
    "ng2-charts": "^3.1.2",
    "ngx-build-plus": "^13.0.1",
    "rxjs": "^6.6.6",
    "sports-ui-sdk": "file:libs/sports-ui-sdk",
    "tslib": "^2.5.0",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.0",
    "@angular-devkit/core": "15.0.0",
    "@angular-devkit/schematics": "15.0.0",
    "@angular-devkit/schematics-cli": "^0.1102.19",
    "@angular-eslint/builder": "1.2.0",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/schematics": "15.1.0",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "15.0.0",
    "@angular/compiler-cli": "15.0.0",
    "@angular/language-service": "15.0.0",
    "@ngxs/devtools-plugin": "^3.8.1",
    "@nrwl/workspace": "16.2.2",
    "@types/clone": "^2.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/lodash": "^4.14.173",
    "@types/node": "^12.20.15",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "autoprefixer": "^10.4.13",
    "codelyzer": "^6.0.0",
    "cypress": "^9.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "madge": "^6.0.0",
    "nx": "16.2.2",
    "postcss-scss": "^4.0.6",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "tailwindcss": "^3.2.6",
    "ts-node": "~8.3.0",
    "typescript": "5.0.4",
    "webpack-bundle-analyzer": "^4.7.0"
  }
